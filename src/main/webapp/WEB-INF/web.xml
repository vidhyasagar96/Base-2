<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Login</servlet-name>
		<display-name>Login</display-name>
		<description></description>
		<servlet-class>com.learnersacademy.servlets.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Logout</servlet-name>
		<display-name>Logout</display-name>
		<description></description>
		<servlet-class>com.learnersacademy.servlets.Logout</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReadStudent</servlet-name>
		<display-name>ReadStudent</display-name>
		<description></description>
		<servlet-class>com.learnersacademy.servlets.ReadStudent</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReadTeacher</servlet-name>
		<display-name>ReadTeacher</display-name>
		<description></description>
		<servlet-class>com.learnersacademy.servlets.ReadTeacher</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ClassDetail</servlet-name>
		<display-name>ClassDetail</display-name>
		<description></description>
		<servlet-class>com.learnersacademy.servlets.ClassDetail</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReadClass</servlet-name>
		<display-name>ReadClass</display-name>
		<description></description>
		<servlet-class>com.learnersacademy.servlets.ReadClass</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReadSubject</servlet-name>
		<display-name>ReadSubject</display-name>
		<description></description>
		<servlet-class>com.learnersacademy.servlets.ReadSubject</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ClassInfo</servlet-name>
		<display-name>ClassInfo</display-name>
		<description></description>
		<servlet-class>com.learnersacademy.servlets.ClassInfo</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReadStudent</servlet-name>
		<url-pattern>/students</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReadTeacher</servlet-name>
		<url-pattern>/teachers</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ClassDetail</servlet-name>
		<url-pattern>/classDetail</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReadClass</servlet-name>
		<url-pattern>/classes</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReadSubject</servlet-name>
		<url-pattern>/subjects</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ClassInfo</servlet-name>
		<url-pattern>/classinfo</url-pattern>
	</servlet-mapping>
	
</web-app>
